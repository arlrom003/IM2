nestedList = []
Row = int(input("Enter row: "))
Col = int(input("Enter col: "))

for x in range(Row):
    print(f"\nRow {x+1}")
    innerList = []
    for y in range(Col):
        num = float(input(f"Enter number{y+1}: "))
        innerList.append(num)
    nestedList.append(innerList)

find = float(input("\nSearch: "))

positions = []
for x in range(Row):
    for y in range(Col):
        if nestedList[x][y] == find:
            positions.append((x+1, y+1))

print("\nThe numbers are:\n")
for row in nestedList:
    for num in row:
        print(num, end=" ")
    print()

if positions:
    pos_dict = {"found": positions}
    for r, c in pos_dict["found"]:
        print(f"Number {find} found at Row {r}, Col {c}")
else:
    print(f"Number {find} not found.")
